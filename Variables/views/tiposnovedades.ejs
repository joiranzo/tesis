<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20,400,0,0" />
    <link rel="stylesheet" href="../../css/motivos.css">
    <link rel="stylesheet" href="../../css/comun.css">
    <title>Tipos de Novedades</title>
</head>

<body>
    <div class="cont-gral">
        <div class="fila_principal">
            <div class="titulo">Tipos Novedades</div>
            <div class="menu_fila col3">
                <a class="material-symbols-outlined bt_azul" href="#" 
                    title="Nuevo">note_add</a>
                <a class="material-symbols-outlined icon bt_azul" href="#"
                    title="Inicio">home</a>
            </div>
        </div>

        <div class="tabla">

            <div class="cabecera">
                <span class="col1">Código</span>
                <span class="col2">Descripión <span>
                
            </div>

            <div class="cuerpo">
                <% resultado.forEach((cliente)=>{ %>
                    <div class="fila">
                        <label class="col1">
                            <%= cliente.IdTipoNovedad%>
                        </label>
                        <label class="col2">
                            <%= cliente.Nombre %>
                        </label> 
                        <div class="menu_fila col3">
                            <a class="material-symbols-outlined bt_verde" href="#"
                                style="display: none;">check_circle </a>    
                            <a class="material-symbols-outlined bt_rojo"  href="#"
                                style="display: none;">cancel<a>
                            <a class="material-symbols-outlined bt_azul"  href="#"
                                title="Editar">Edit</a>    
                            <a class="material-symbols-outlined bt_rojo"  href="#"
                                title="Eliminar">Delete</a>        
                        </div>
                    </div>    
                <% }); %>
    
            </div>
        </div>

    </div>
</body>
<script src="../../js/TiposNovedades.js"></script>

</html>